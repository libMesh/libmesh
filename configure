              CXXFLAGS_DEVEL="$CXXFLAGS_DBG"
              CFLAGS_DBG="$CFLAGS_DBG -w1 -g -inline_debug_info -wd266 -wd1572"
              CFLAGS_OPT="$CFLAGS_OPT -O2 -Ob2 $INTEL_AX_FLAG -unroll -w0 -vec_report0 -par_report0 -openmp_report0"
              CFLAGS_DEVEL="$CFLAGS_DBG"
              ;;

          # Intel ICC < v8.1
          intel_icc*)
