            hardcode_libdir_flag_spec_F77='-L$libdir'
            hardcode_minus_L_F77=yes
        ;;
      esac
      ;;

    bsdi[45]*)
      export_dynamic_flag_spec_F77=-rdynamic
      ;;

    cygwin* | mingw* | pw32* | cegcc*)
