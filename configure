          fi
          ;;

      # All Intel ICC/ECC flavors
      intel_*)

        # Intel understands the gcc-like no-deprecated flag
        NODEPRECATEDFLAG="-Wno-deprecated"

        # Intel compilers use -qp for profiling
        PROFILING_FLAGS="-qp"

